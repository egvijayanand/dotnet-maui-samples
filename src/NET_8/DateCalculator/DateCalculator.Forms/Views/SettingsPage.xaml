<?xml version="1.0" encoding="utf-8" ?>
<views:BasePage
    x:Class="DateCalculator.Views.SettingsPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:fas="clr-namespace:FontAwesome.Solid;assembly=VijayAnand.FontAwesome"
    xmlns:views="clr-namespace:DateCalculator.Views"
    xmlns:vm="clr-namespace:DateCalculator.ViewModels;assembly=DateCalculator.Shared"
    Title="{Binding Title}"
    x:DataType="vm:SettingsViewModel">
    <ContentPage.BindingContext>
        <vm:SettingsViewModel />
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Button
                Clicked="Home_Clicked"
                HorizontalOptions="CenterAndExpand"
                Text="Home"
                VerticalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</views:BasePage>
